{
  "log": {
    "path": "./logs", 
    "maxfile": 10000, 
    "backup": 7, 
    "level": "info"
  }, 
  "auth": {
    "knownHosts": ".ssh/known_hosts", 
    "passwd": "", 
    "privateKey": ".ssh/id_rsa", 
    "user": ""
  }, 
